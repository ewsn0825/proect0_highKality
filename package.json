{
  "name": "project-template",
  "scripts": {
    "start": "run-p watch server",
    "sass": "sass src/scss:src/style",
    "watch": "npm run sass -- --watch",
    "server": "live-server src",
    "clear": "rimraf src/style",
    "cssbuild": "postcss src/style/index.css -o src/style/main.css",
    "build": "parcel build ./src/index.html --dist-dir build"
  },
  "dependencies": {
    "cssnano": "^6.0.1",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.12.0",
    "postcss": "^8.4.24",
    "postcss-cli": "^10.1.0",
    "postcss-combine-media-query": "^1.0.1",
    "rimraf": "^5.0.1",
    "sass": "^1.63.5"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "tailwindcss": "^3.4.3"
  }
}
